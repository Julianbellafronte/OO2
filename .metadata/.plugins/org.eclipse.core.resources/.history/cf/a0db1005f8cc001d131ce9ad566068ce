package ar.edu.unlp.info.oo2.Ejercicio_1;

public class User {
	ArrayList tws[];
	String screenName;
	
	public User(String screenName) {
		this.tws = new Tweet[]
		this.screenName = screenName;
	}
	public Tweet[] getTws() {
		return tws;
	}
	public void setTws(Tweet[] tws) {
		this.tws = tws;
	}
	public String getScreenName() {
		return screenName;
	}
	public void setScreenName(String screenName) {
		this.screenName = screenName;
	}
	
	
	
}
